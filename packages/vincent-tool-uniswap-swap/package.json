{
  "name": "@lit-protocol/vincent-tool-uniswap-swap",
  "version": "0.1.0-0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@lit-protocol/vincent-contracts": "0.1.1-3",
    "@lit-protocol/vincent-tool": "workspace:*",
    "ethers": "^5.7.2",
    "tslib": "^2.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.39.0",
    "@lit-protocol/auth-helpers": "^7.0.8",
    "@lit-protocol/constants": "^7.0.8",
    "@lit-protocol/contracts-sdk": "^7.0.6",
    "@lit-protocol/lit-auth-client": "^7.0.6",
    "@lit-protocol/lit-node-client": "^7.0.8",
    "esbuild": "^0.19.11",
    "node-fetch": "^2.7.0",
    "ts-node": "^10.9.2",
    "viem": "^2.23.2"
  },
  "type": "commonjs",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "test:contract-caller": "npx ts-node tools/scripts/vincent-contract-caller.ts"
  },
  "nx": {
    "sourceRoot": "packages/vincent-tool-uniswap-swap/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/vincent-tool-uniswap-swap/dist",
          "main": "packages/vincent-tool-uniswap-swap/src/index.ts",
          "tsConfig": "packages/vincent-tool-uniswap-swap/tsconfig.lib.json",
          "assets": [
            "packages/vincent-tool-uniswap-swap/*.md"
          ]
        }
      },
      "build:action": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "build"
        ],
        "options": {
          "commands": [
            "node tools/scripts/build-lit-action.js"
          ],
          "cwd": "packages/vincent-tool-uniswap-swap",
          "parallel": false
        },
        "outputs": [
          "{workspaceRoot}/packages/vincent-tool-uniswap-swap/dist/deployed-lit-action.js"
        ]
      },
      "deploy": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "build:action"
        ],
        "options": {
          "commands": [
            "node tools/scripts/deploy-lit-action.js"
          ],
          "cwd": "packages/vincent-tool-uniswap-swap"
        }
      },
      "publish": {
        "executor": "@nx/js:npm-publish",
        "dependsOn": [
          "deploy"
        ],
        "options": {
          "packageRoot": "dist"
        }
      }
    },
    "name": "vincent-tool-uniswap-swap"
  }
}