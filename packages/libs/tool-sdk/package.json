{
  "name": "@lit-protocol/vincent-tool-sdk",
  "version": "1.0.1",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@lit-protocol/lit-action-bundler-rollup": "workspace:*",
    "ethers": "5.8.0",
    "tslib": "^2.8.1",
    "zod": "^3.25.64"
  },
  "scripts": {
    "watch:type-tests": "chokidar 'src/**/*' --initial --await-write-finish --delay 100 --silent -c './scripts/run-typecheck.sh'",
    "typecheck": "./scripts/precommit-check.sh"
  },
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.cjs.js",
      "require": "./dist/index.cjs.js"
    },
    "./internal": {
      "import": "./dist/src/internal.esm.js",
      "default": "./dist/src/internal.cjs.js",
      "require": "./dist/src/internal.cjs.js"
    }
  },
  "files": [
    "dist/**",
    "*.md"
  ],
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "typedoc": "0.27.9",
    "typedoc-material-theme": "1.3.0",
    "typedoc-plugin-extras": "^4.0.0",
    "typedoc-plugin-zod": "^1.4.1"
  }
}
