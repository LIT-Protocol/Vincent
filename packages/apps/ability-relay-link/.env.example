# E2E Test Environment Variables
# Copy this file to .env and fill in your values

# Required: Private keys for test wallets
TEST_FUNDER_PRIVATE_KEY=0x...
TEST_APP_MANAGER_PRIVATE_KEY=0x...
TEST_APP_DELEGATEE_PRIVATE_KEY=0x...
TEST_USER_EOA_PRIVATE_KEY=0x...

# Required: ZeroDev project ID for bundler operations
ZERODEV_PROJECT_ID=your_zerodev_project_id

# Optional: RPC and API URLs
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
BASE_MAINNET_RPC_URL=https://mainnet.base.org

VINCENT_API_URL=https://api.heyvincent.ai

# Build Configuration
# Vincent registry chain - which chain the Vincent registry contract is deployed on
# Options: 'base' (Base Mainnet) or 'baseSepolia' (Base Sepolia testnet)
# For E2E tests, set this to 'baseSepolia'
VINCENT_REGISTRY_LIT_CHAIN=baseSepolia
