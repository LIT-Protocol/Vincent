{
  "name": "dashboard",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/dashboard",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/dashboard",
        "command": "next build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/dashboard",
        "command": "next dev"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/dashboard",
        "command": "next lint"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/dashboard",
        "command": "tsc --noEmit"
      }
    }
  },
  "tags": []
}