{
  "$schema": "https://typedoc.org/schema.json",
  "readme": "docs/Why-Vincent.md",
  "entryPoints": [
    "./src/index.ts"
  ],
  "projectDocuments": [
    "docs/Why-Vincent.md",
    "docs/Developers/Quick-Start.md",
    "docs/Developers/Custom-Tools.md",
    "docs/Users/Onboarding.md",
    "docs/Contact-Us.md"
  ],
  "out": "./sdk-docs",
  "skipErrorChecking": true,
  "plugin": ["typedoc-github-theme"],
  "navigation": {
    "includeCategories": true
  },
  "defaultCategory": "API",
  "categorizeByGroup": false,
  "categoryOrder": ["Intro", "Developers", "Users", "Contact", "Vincent SDK API", "Vincent Web App",  "Vincent Tools"],
  "router": "category",
  "visibilityFilters": { },
  "includeVersion": true,
  "externalSymbolLinkMappings": {
    "@lit-protocol/types" : {
      "*": "https://v7-api-doc-lit-js-sdk.vercel.app/modules/types_src.html"
    },
    "@lit-protocol/pkp-ethers" : {
      "*": "https://v7-api-doc-lit-js-sdk.vercel.app/modules/pkp_ethers_src.html"
    },
    "did-jwt" : {
      "*": "https://www.jsdocs.io/package/did-jwt"
    },
    "ethers" : {
      "*": "https://docs.ethers.org/v5/api/"
    },
    "@ethersproject/abstract-signer" : {
      "*": "https://docs.ethers.org/v5/api/"
    }
  }
}
