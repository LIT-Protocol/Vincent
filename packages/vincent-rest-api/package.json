{
  "name": "@lit-protocol/vincent-rest-api",
  "version": "1.0.0",
  "description": "Integration tests for Vincent Registry API",
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "generate-openapi": "pnpx ts-node src/utils/generate-openapi.ts",
    "generate-client": "pnpx openapi-typescript-codegen --input src/generated/openapi.yaml --name Api --postfixServices Api --indent 2 --output src/generated/api",
    "generate-api": "run-s generate-openapi generate-client"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.5",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.25.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "unbuild": "^1.0.0"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^5.5.0",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "zod": "^3.22.2"
  }
}
