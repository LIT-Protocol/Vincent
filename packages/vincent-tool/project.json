{
  "name": "vincent-tool",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/vincent-tool/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/vincent-tool/dist",
        "main": "packages/vincent-tool/src/index.ts",
        "tsConfig": "packages/vincent-tool/tsconfig.lib.json",
        "assets": ["packages/vincent-tool/*.md"],
        "formats": ["cjs"],
        "skipTypeCheck": true,
        "generateExportsField": true,
        "updateBuildableProjectDepsInPackageJson": true,
        "buildableProjectDepsInPackageJsonType": "dependencies"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/vincent-tool/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/vincent-tool/jest.config.js"
      }
    }
  },
  "tags": []
} 