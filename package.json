{
  "name": "@lit-protocol/vincent-source",
  "version": "0.0.0",
  "description": "Lit Protocol Vincent SDK",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LIT-Protocol/Vincent.git"
  },
  "homepage": "https://github.com/LIT-Protocol/Vincent#readme",
  "bugs": {
    "url": "https://github.com/LIT-Protocol/Vincent/issues"
  },
  "keywords": [
    "library"
  ],
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "format": "nx format:write",
    "release": "nx release",
    "dev:dashboard": "nx serve dashboard",
    "build:dashboard": "nx build dashboard",
    "start:dashboard": "nx serve dashboard --prod",
    "lint:dashboard": "nx lint dashboard",
    "start:docs": "nx run vincent-sdk:typedoc",
    "install:contracts": "cd packages/vincent-contracts && forge install",
    "make:contracts": "cd packages/vincent-contracts && make",
    "gen:contracts": "bun run make:contracts get-abis run-postbuild-scripts",
    "build:contracts": "nx build vincent-contracts",
    "clean": "node tools/clean.js",
    "deploy:tools": "npx nx deploy vincent-tool-uniswap-swap",
    "prepare": "husky"
  },
  "dependencies": {
    "@lit-protocol/flags": "^1.0.0",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.44.2",
    "@eslint/js": "^9.25.1",
    "@nx/esbuild": "20.8.0",
    "@nx/eslint": "20.8.0",
    "@nx/eslint-plugin": "20.8.0",
    "@nx/jest": "20.8.0",
    "@nx/js": "20.8.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "bun": "^1.2.5",
    "esbuild": "^0.19.2",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.5.1",
    "nx": "20.8.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "ts-node": "10.9.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  },
  "overrides": {
    "typescript": "5.8.3"
  }
}
